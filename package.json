{
  "name": "anode",
  "version": "0.2.0",
  "type": "module",
  "description": "Anode - real-time collaborative notebook system with rich outputs",
  "engines": {
    "node": ">=23.0.0"
  },
  "scripts": {
    "dev": "vite --port 5173",
    "dev:sync": "wrangler dev",
    "build": "tsc && vite build",
    "build:prod": "NODE_ENV=production tsc && vite build",
    "build:dev": "NODE_ENV=development tsc && vite build",
    "build:sync": "wrangler deploy --dry-run",
    "deploy": "vite build && wrangler deploy",
    "deploy:production": "test -f .env.production || (echo 'Error: .env.production file is required for production deployment' && exit 1) && pnpm build:prod && wrangler pages deploy dist --project-name anode --commit-dirty=true",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:integration": "vitest run test/integration/",
    "test:debug": "DEBUG_TESTS=true vitest run",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "check": "pnpm type-check && pnpm lint",
    "start": "echo 'Use pnpm dev for web client and pnpm dev:sync for sync worker in separate terminals'"
  },
  "dependencies": {
    "@livestore/adapter-web": "^0.3.1",
    "@livestore/livestore": "^0.3.1",
    "@livestore/react": "^0.3.1",
    "@livestore/sync-cf": "^0.3.1",
    "@livestore/wa-sqlite": "1.0.5-dev.2",
    "@livestore/webmesh": "^0.3.1",
    "@overengineering/fps-meter": "^0.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@runt/schema": "jsr:^0.2.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "ansi-to-react": "^6.1.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.514.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "strip-ansi": "^7.1.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.4",
    "effect": "3.15.5"
  },
  "devDependencies": {
    "@livestore/adapter-node": "^0.3.1",
    "@cloudflare/workers-types": "^4.20250106.0",
    "@effect/vitest": "0.23.7",
    "@livestore/devtools-vite": "^0.3.1",
    "@tailwindcss/cli": "^4.1.10",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/vite": "^4.1.10",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/ui": "^3.0.0",
    "autoprefixer": "^10.4.21",
    "happy-dom": "^15.11.6",
    "postcss": "^8.5.5",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.0.0",
    "wrangler": "4.19.2"
  },
  "pnpm": {
    "overrides": {
      "effect": "3.15.5",
      "react": "19.0.0",
      "react-dom": "19.0.0",
      "@effect/platform": "0.82.4",
      "@effect/typeclass": "0.34.2",
      "@effect/cluster": "0.34.2",
      "@effect/experimental": "0.46.8",
      "@effect/sql": "0.35.8",
      "@effect/rpc": "0.59.9"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "msgpackr-extract",
      "sharp",
      "workerd"
    ]
  },
  "keywords": [
    "anode",
    "collaborative",
    "notebook",
    "real-time",
    "livestore",
    "event-sourcing",
    "python",
    "jupyter",
    "workspace",
    "rich-outputs",
    "matplotlib",
    "pandas",
    "svg"
  ],
  "author": "Anode Team",
  "license": "MIT"
}
