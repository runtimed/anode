{
  "name": "@runtimed/schema",
  "version": "0.1.0",
  "type": "module",
  "description": "Schema definitions for Anode - event-sourced notebook system",
  "exports": {
    ".": {
      "deno": "./src/index.ts",
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "development": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:check": "eslint src/ --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "@livestore/livestore": "^0.3.1",
    "@runt/schema": "jsr:^0.13.0",
    "effect": "3.15.5",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "typescript": "^5.8.3",
    "prettier": "^3.6.0",
    "eslint": "^9.30.1",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1"
  },
  "engines": {
    "node": ">=23.0.0",
    "pnpm": ">=10.9.0"
  },
  "keywords": [
    "schema",
    "event-sourcing",
    "livestore",
    "notebook",
    "runt",
    "anode"
  ],
  "author": "Runt Team",
  "license": "MIT"
}
