{
  "name": "anode-iframe-outputs",
  "version": "0.2.0",
  "private": true,
  "description": "Isolated iframe content server for Anode user-generated outputs",
  "type": "module",
  "engines": {
    "node": ">=23.0.0",
    "pnpm": ">=10.9.0"
  },
  "scripts": {
    "dev": "vite --port 8000",
    "build": "vite build",
    "preview": "vite preview",
    "test": "http-server . -p 3000 -o test-react.html",
    "wrangler:dev": "wrangler dev",
    "deploy": "wrangler deploy --env production",
    "deploy:production": "wrangler deploy --env production",
    "deploy:preview": "wrangler deploy --env preview",
    "deploy:staging": "wrangler deploy --env staging",
    "logs": "wrangler tail --env production",
    "logs:preview": "wrangler tail --env preview"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250106.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "http-server": "^14.1.1",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "wrangler": "4.25.0"
  },
  "keywords": [
    "iframe",
    "sandbox",
    "user-content",
    "cloudflare-worker"
  ],
  "author": "Runt Team",
  "license": "MIT"
}
